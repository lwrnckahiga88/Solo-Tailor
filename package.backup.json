{
  "name": "@xenova/transformers",
  "version": "2.17.2",
  "description": "State-of-the-art Machine Learning for the web. Run ðŸ¤— Transformers directly in your browser, with no need for a server!",
  "main": "src/index.cjs",
  "types": "./types/transformers.d.ts",
  "type": "module",
  "scripts": {
   "start": "concurrently \"C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe -Command yarn webpack:dev\" \"C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe -Command yarn backend:start\",
    "webpack:dev": "webpack serve --open",
    "backend:start": "node src/index.cjs",
    "typegen": "tsc src/webpack.config.js --allowJs --declaration --emitDeclarationOnly --declarationMap --outDir types",
    "dev": "webpack serve --no-client-overlay",
    "build": "webpack && npm run typegen",
    "generate-tests": "python -m tests.generate_tests",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose --maxConcurrency 1",
    "readme": "python ./docs/scripts/build_readme.py",
    "docs-api": "node ./docs/scripts/generate.js",
    "docs-preview": "doc-builder preview transformers.js ./docs/source/ --not_python_module",
    "docs-build": "doc-builder build transformers.js ./docs/source/ --not_python_module --build_dir ./docs/build/ --repo_owner xenova",
    "libp2p:start": "node ./src/index.js",
    "libp2p:build": "tsc && esbuild ./dist/server.js --bundle --outfile=dist/bundle.js --platform=node --external:libp2p --external:libp2p-noise --external:libp2p-mplex --external:libp2p-webrtc-star --external:multiformats/cid",
    "libp2p:dev": "ts-node src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xenova/transformers.js.git"
  },
  "keywords": [
    "transformers",
    "transformers.js",
    "huggingface",
    "hugging face",
    "machine learning",
    "deep learning",
    "artificial intelligence",
    "AI",
    "ML"
